<?xml version="1.0" encoding="UTF-8"?>
<!-- Hibernate XML Mapping File -->
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="rs.eventbroker.db.token.TokenBO" table="eb_tokens" dynamic-insert="false" dynamic-update="false" batch-size="300">
		<id name="id" column="uid" type="int">
			<generator class="native" />
		</id>

		<property name="token" type="string" column="token" not-null="true"/>
		<property name="expiryDate" type="RsDate" column="expiry_date" not-null="true"/>
		<property name="permissions" type="string" column="permissions" not-null="false"/>
	</class>
</hibernate-mapping>
